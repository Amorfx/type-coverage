includes:
    - config/extension.neon

parameters:
    # parameters of this extension
    type_coverage:
        return_type: 0
        param_type: 0
        property_type: 0

    level: 8

    paths:
        - src
        - tests

    checkGenericClassInNonGenericObjectType: false

    excludePaths:
        - "*/Fixture/*"
        - "*/Source/*"
